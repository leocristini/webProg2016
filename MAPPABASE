<body>
    <div id="map"></div>
    <script type="text/javascript" src="http(s)://www.google.com/jsapi"></script>
    <script>
      function initMap() {
        var posx = {lat: 46.06941967 , lng: 11.12015963};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 4,
          center: posx
        });
        var marker = new google.maps.Marker({
          position: posx,
          map: map
        });
      }
    </script>
    <script async defer
        src="https://maps.googleapis.com/maps/api/js?key= AIzaSyC-e_df6T64QZRUmRNPgeryz8DsWRPvjcM&callback=initMap">
    </script>
  </body>
